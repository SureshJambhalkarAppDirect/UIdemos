# Adobe VIP Marketplace API Configuration
# Copy this file to .env and fill in your actual credentials

# Adobe OAuth Client Credentials
ADOBE_CLIENT_ID=your_client_id_here
ADOBE_CLIENT_SECRET=your_client_secret_here
ADOBE_TECHNICAL_ACCOUNT_ID=your_technical_account_id_here
ADOBE_ORGANIZATION_ID=your_organization_id_here
ADOBE_RESELLER_ID=your_reseller_id_here
ADOBE_DISTRIBUTOR_ID=your_distributor_id_here

# Adobe Private Key (entire private key including BEGIN/END lines)
ADOBE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----
your_private_key_content_here
-----END PRIVATE KEY-----"

# Adobe API Endpoints
ADOBE_API_URL=https://partnersandbox-stage.adobe.io
ADOBE_IMS_URL=https://ims-na1-stg1.adobelogin.com
